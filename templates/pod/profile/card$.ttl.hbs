@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix solid: <http://www.w3.org/ns/solid/terms#>.
@prefix space: <http://www.w3.org/ns/pim/space#>.
<>
    a foaf:PersonalProfileDocument;
    foaf:maker <{{webId}}>;
    foaf:primaryTopic <{{webId}}>.

<{{webId}}>
    {{#if name}}foaf:name "{{name}}";{{/if}}
    space:preferencesFile <{{podBaseUrl}}settings/prefs>;
    solid:privateTypeIndex <{{podBaseUrl}}settings/privateTypeIndex>;
    solid:publicTypeIndex <{{podBaseUrl}}settings/publicTypeIndex>;
    {{#if oidcIssuer}}solid:oidcIssuer <{{oidcIssuer}}>;{{/if}}
    a foaf:Person.
